if __name__ == '__main__':
    n = int(input())
    msum=0
    student_marks = {}
    for _ in range(n):
        name, *line = input().split()
        scores = list(map(float, line))
        student_marks[name] = scores
    query_name = input()
    for i in student_marks[query_name]:
        msum+=i
    a=msum/3
    print(f'{a:.2f}')
